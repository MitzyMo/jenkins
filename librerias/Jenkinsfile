@Library('jenkinsLibs') _
def numPrue = BUILD_NUMBER
pipeline {
    agent any
    stages {
        stage('crear'){
            steps{
                script{
                    mover.crearDir(numPrue)
                }
            }
        }
        stage('copiar'){
            steps{
                script{
                    echo "Procedemos a copiar"
                    mover.clonar()
                }
            }
        }
        stage('mover'){
            steps{
                script{
                    echo "llegamos hasta aca"
                }
            }
        }
    }
}