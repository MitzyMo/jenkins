@Library('jenkinsLibs') _
def numPrue = BUILD_NUMBER
pipeline {
    agent any
    stages {
        stage('crear'){
            steps{
                script{
                    echo "Creando carpeta..."
                    mover.crearDir(numPrue)
                }
            }
        }
        stage('copiar'){
            steps{
                script{
                    echo "Clonar del repo..."
                    mover.clonar()
                }
            }
        }
        stage('mover'){
            steps{
                script{
                    echo "moviendo a documentos..."
                    mover.mover()
                }
            }
        }
    }
}